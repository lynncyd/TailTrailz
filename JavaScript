let map;

async function initMap() {
  const { Map } = await google.maps.importLibrary("maps");

  map = new Map(document.getElementById("map"), {
    center: {lat: 33.7290, lng: -84.7316},
    zoom: 8,
  });
}

  // Call the function to add markers
  addPetMarkers();

  // Call the function to add a County Boundary
  addcountyboundary(); 
}

function addPetMarkers() {
  var pets = [
    {lat: 33.679996, lng: -84.681955, name: "Roxy"}
    // Add more pets here
  ];

  pets.forEach(function(pet) {
    var marker = new google.maps.Marker({
      position: {lat: pet.lat, lng: pet.lng},
      map: map,
      title: pet.name // The name will appear as a tooltip on hover
    });
  });
}
